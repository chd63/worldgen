#version 330 core
out vec4 FragColor;

in vec4 vertexColor;
in vec3 position;
in vec2 TexCoord;

uniform sampler2D texture1;
uniform sampler2D texture2;
uniform sampler2D texture3;

void main()
{
	if(position.z > 25.0f)
	{
		FragColor = texture(texture2, TexCoord);
		return;
	}
	if(position.z > 5.0f)
	{
		FragColor = texture(texture1, TexCoord);
	}
	else
	{
		FragColor = texture(texture3, TexCoord);
	}

}